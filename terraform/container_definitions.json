[
  {
    "name": "backend",
    "image": "backend:latest",
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "backend",
        "awslogs-group": "/ecs/backend"
      }
    },
    "portMappings": [
      {
        "protocol": "tcp",
        "containerPort": 8080
      }
    ]
  },
  {
    "name": "frontend",
    "image": "frontend:latest",
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-region": "ap-northeast-1",
        "awslogs-stream-prefix": "frontend",
        "awslogs-group": "/ecs/frontend"
      }
    },
    "portMappings": [
      {
        "protocol": "tcp",
        "containerPort": 3000
      }
    ]
  }
]
